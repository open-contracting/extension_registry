{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "extension",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "Id": {
      "type": "string",
      "pattern": "^[A-Za-z_]+$"
    },
    "Category": {
      "type": "string",
      "enum": [
        "amendment",
        "award",
        "bids",
        "budget",
        "contract",
        "document",
        "eu",
        "implementation",
        "item",
        "milestones",
        "package",
        "parties",
        "partyDetail",
        "period",
        "planning",
        "ppp",
        "release",
        "tender",
        "transaction",
        "value"
      ]
    },
    "Core": {
      "type": "string",
      "enum": [
        "true"
      ]
    }
  },
  "required": [
    "Id"
  ]
}
