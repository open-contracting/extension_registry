{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "extension",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[a-zA-Z0-9_]*$"
    },
    "repository_url": {
      "type": "string",
      "pattern": "^https:\/\/github.com\/[^\/]*\/[^\/]*[\/]?$"
    },
    "category": {
      "type": "string",
      "enum": []
    },
    "core": {
      "type": "string",
      "enum": ["True", "False"]
    }
  },
  "required": ["id", "repository_url", "category", "core"]
}
