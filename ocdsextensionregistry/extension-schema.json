{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "extension",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "Id": {
      "type": "string",
      "pattern": "^[a-zA-Z_]+$"
    },
    "RepositoryURL": {
      "type": "string",
      "format": "uri"
    },
    "Category": {
      "type": "string",
      "enum": [
        "contract",
        "ppp",
        "bids",
        "tender",
        "implementation",
        "parties",
        "release",
        "item",
        "partyDetail",
        "milestones"
      ]
    },
    "Core": {
      "type": "string",
      "enum": [
        "True",
        "False"
      ]
    }
  },
  "required": [
    "Id",
    "RepositoryURL",
    "Category",
    "Core"
  ]
}
